<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xuanyu.stickyheader.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.OffsetStickyHeaderActivity"></activity>
        <activity android:name=".activity.ComplexViewStickyHeaderActivity"></activity>
        <activity android:name=".activity.ScrollToPositionStickyHeaderActivity"></activity>
        <activity android:name=".activity.AppBarLayoutStickyHeaderActivity"></activity>
        <activity android:name=".activity.CustomTopStickyHeaderActivity"></activity>
        <activity android:name=".activity.MultiStickyHeaderActivity"></activity>
        <activity android:name=".activity.PositionStickyHeaderActivity"></activity>
        <activity android:name=".activity.ItemDecorationStickyHeaderActivity"></activity>
        <activity android:name=".activity.SwitchStickyHeaderActivity" />
        <activity android:name=".activity.SmallHeaderViewStickyHeaderActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SingleStickyHeaderActivity" />
    </application>

</manifest>